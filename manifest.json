{
    "manifest_version": 2,
    "name": "Linkedin learning Video Downloader",
    "version": "1.1",
    "description": "blabal",
    "homepage_url": "https://github.com/",
    "icons": {
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png"
    },

    "permissions": [
        "*://www.linkedin.com/learning/*",
        "activeTab",
        "tabs",
        "downloads",
        "storage",
        "unlimitedStorage"
    ],

    "browser_action": {
      "default_icon": "icons/icon-48.png",
      "default_title": "Linkedin learning Video Downloader"
    },

    "background": {
      "scripts": ["background.js"]
    }

  }
