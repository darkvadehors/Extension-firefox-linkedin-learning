{
    "manifest_version": 2,
    "name": "Linkedin learning Video Downloader",
    "version": "1.0.0",
    "description": "Download all the videos of a Linkedin Learning Premium training",
    "homepage_url": "https://github.com/darkvadehors/Extension-firefox-linkedin-learning",
    "icons": {
        "32": "img/icons/icon-green.svg",
        "48": "img/icons/icon-green.svg",
        "96": "img/icons/icon-green.svg"
    },
    "permissions": [
        "<all_urls>",
        "activeTab",
        "downloads",
        "webNavigation",
        "webRequest",
        "webRequestBlocking"
    ],
    "browser_action": {
        "default_icon": "img/icons/icon-green.svg",
        "default_title": "Linkedin learning Video Downloader"
    },
    "background": {
        "scripts": [
            "/content_scripts/background.js"
        ]
    }
}