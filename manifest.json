{
    "manifest_version": 2,
    "name": "Linkedin learning Video Downloader",
    "version": "1.0",
    "description": "Download all the videos of a Linkedin Learning Premium training",
    "homepage_url": "https://johnben.ch/",
    "icons": {
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png"
    },
    "permissions": [
        "<all_urls>",
        "activeTab",
        "tabs",
        "downloads"
    ],
    "browser_action": {
        "default_icon": "icons/icon-48.png",
        "default_title": "Linkedin learning Video Downloader"
    },
    "background": {
        "scripts": [
            "/content_scripts/background.js"
        ]
    }
}